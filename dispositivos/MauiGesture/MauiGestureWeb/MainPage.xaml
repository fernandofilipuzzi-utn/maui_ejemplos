<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiGestureWeb.MainPage">

    <StackLayout>
        <RefreshView x:Name="refreshView"  Loaded="refreshView_Loaded"  Refreshing="refreshView_Refreshing">
            <WebView x:Name="Navegador" Navigating="Navegador_Navigating"  
                                Navigated="Navegador_Navigated" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" ></WebView>
        </RefreshView>

        <Grid x:Name="Mensage" IsVisible="false">
        </Grid>

    </StackLayout>
    
</ContentPage>
